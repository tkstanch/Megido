{
  "name": "megido-security",
  "version": "2.0.0",
  "description": "Megido Security Testing Platform - UI with Tailwind CSS",
  "scripts": {
    "build:css": "npx tailwindcss -i ./static/css/tailwind.input.css -o ./static/css/tailwind.output.css --minify",
    "watch:css": "npx tailwindcss -i ./static/css/tailwind.input.css -o ./static/css/tailwind.output.css --watch",
    "build:tsx": "webpack --config webpack.config.js",
    "build:all": "npm run build:css && npm run build:tsx",
    "dev": "npm run watch:css"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.17",
    "css-loader": "^6.8.1",
    "postcss": "^8.4.35",
    "postcss-loader": "^7.3.3",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3.4.19",
    "ts-loader": "^9.5.0",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "keywords": [
    "security",
    "pentesting",
    "django",
    "tailwindcss"
  ],
  "author": "Megido Security Team",
  "license": "MIT",
  "main": "postcss.config.js",
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tkstanch/Megido.git"
  },
  "bugs": {
    "url": "https://github.com/tkstanch/Megido/issues"
  },
  "homepage": "https://github.com/tkstanch/Megido#readme"
}
